<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.plataformaam.mobile.clientefinal.userinterfaces.listfragments.FragmentVComCompositeList"
    android:background="@drawable/plano_fundo_jardim_penha"
    >

    <ListView android:id="@+id/vcom_list" android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="5sp"
        android:layout_margin="5sp"
        android:divider="@null"
        android:showDividers="end"
        android:dividerHeight="5.0sp"
        />

    <TextView android:id="@android:id/empty" android:layout_width="match_parent"
        android:layout_height="match_parent" android:gravity="center" />

</FrameLayout>
