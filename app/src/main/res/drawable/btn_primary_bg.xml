<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#AA449def" />
            <stroke android:width="1dp" android:color="#AA2f6699" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="#AA2f6699" android:startColor="#AA449def" />
            <stroke android:width="1dp" android:color="#AA2f6699" />
            <corners android:radius="5dp" />
        </shape>
    </item>
</selector>